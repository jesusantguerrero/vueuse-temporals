{
  "name": "vueuse-temporals",
  "version": "1.4.3",
  "description": "Date related composables for atmosphere-ui",
  "repository": "https://github.com/Tahul/vueuse-temporals",
  "bugs": {
    "url": "https://github.com/Tahul/vueuse-temporals/issues"
  },
  "homepage": "https://github.com/Tahul/vueuse-temporals#readme",
  "keywords": [
    "vue",
    "composable"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./useDatePager": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "dev": "vitest dev",
    "dev:demo": "vite",
    "build": "unbuild",
    "build:demo": "vite build",
    "release": "npm run build && bumpp --commit --tag --push && npm publish",
    "lint": "eslint \"**/*.{vue,ts,js}\"",
    "lint:fix": "npm run lint --write",
    "test": "npm run test:types && npm run test:unit && npm run test:coverage",
    "test:types": "tsc --build tsconfig.json",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@antfu/eslint-config": "^0.40.2",
    "@types/luxon": "^3.3.1",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-v8": "^0.34.1",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vuedx/typecheck": "^0.7.6",
    "@vuedx/typescript-plugin-vue": "^0.7.6",
    "bumpp": "^9.1.1",
    "c8": "^8.0.1",
    "date-fns": "^2.30.0",
    "eslint": "^8.47.0",
    "lint-staged": "^14.0.0",
    "luxon": "^3.4.0",
    "pascalcase": "^2.0.0",
    "prettier": "^3.0.2",
    "typescript": "^5.1.6",
    "unbuild": "^1.2.1",
    "vite": "^4.4.9",
    "vitest": "^0.34.1",
    "vue": "^3.3.4",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "author": "Jesus Guerrero <jesusant.guerrero@gmail.com>",
  "license": "MIT"
}
